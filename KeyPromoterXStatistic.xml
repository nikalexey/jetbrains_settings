<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Back">
        <Item>
          <IdeaActionID>Back</IdeaActionID>
          <ShortCut>'Ctrl+Alt+Left' or 'Button4 Click'</ShortCut>
          <Description>Back</Description>
          <Count>8</Count>
        </Item>
      </Statistic>
      <Statistic Action="Build">
        <Item>
          <IdeaActionID>Build</IdeaActionID>
          <ShortCut>'Ctrl+F9'</ShortCut>
          <Description>Build</Description>
          <Count>8</Count>
        </Item>
      </Statistic>
      <Statistic Action="Copy Reference">
        <Item>
          <IdeaActionID>CopyReference</IdeaActionID>
          <ShortCut>'Ctrl+Alt+Shift+C'</ShortCut>
          <Description>Copy Reference</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Evaluate Expression...">
        <Item>
          <IdeaActionID>EvaluateExpression</IdeaActionID>
          <ShortCut>'Alt+F8'</ShortCut>
          <Description>Evaluate Expression...</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Expand">
        <Item>
          <IdeaActionID>ExpandRegion</IdeaActionID>
          <ShortCut>'Ctrl+NumPad +' or 'Ctrl+Equals'</ShortCut>
          <Description>Expand</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Open Task...">
        <Item>
          <IdeaActionID>tasks.goto</IdeaActionID>
          <ShortCut>'Alt+Shift+N'</ShortCut>
          <Description>Open Task...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Open...">
        <Item>
          <IdeaActionID>OpenFile</IdeaActionID>
          <ShortCut>'Ctrl+O'</ShortCut>
          <Description>Open...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'TestCommandDocumentS...'">
        <Item>
          <IdeaActionID>RunClass</IdeaActionID>
          <ShortCut>'Ctrl+F9'</ShortCut>
          <Description>Run 'TestCommandDocumentS...'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run...">
        <Item>
          <IdeaActionID>ChooseRunConfiguration</IdeaActionID>
          <ShortCut>'Alt+Shift+F10'</ShortCut>
          <Description>Run...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Settings...">
        <Item>
          <IdeaActionID>ShowSettings</IdeaActionID>
          <ShortCut>'Ctrl+Alt+S'</ShortCut>
          <Description>Settings...</Description>
          <Count>24</Count>
        </Item>
      </Statistic>
      <Statistic Action="Show Execution Point">
        <Item>
          <IdeaActionID>ShowExecutionPoint</IdeaActionID>
          <ShortCut>'Alt+NumPad *' or 'Ctrl+L'</ShortCut>
          <Description>Show Execution Point</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Synchronize">
        <Item>
          <IdeaActionID>Synchronize</IdeaActionID>
          <ShortCut>'Ctrl+Alt+Y'</ShortCut>
          <Description>Synchronize</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Toggle Line Breakpoint">
        <Item>
          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'F9'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>